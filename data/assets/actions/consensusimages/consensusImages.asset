local consensusImages = {
    Identifier = "os.consensusImages",
    Name = "Turn on and off Primates Consensus images",
    Command = [[
      if (openspace.getPropertyValue("Scene.primates_consensus_species1.Renderable.Opacity") == 1.0) then
        openspace.setPropertyValue("Scene.primates_consensus_species*.Renderable.Opacity", 0.0)
      else
        openspace.setPropertyValue("Scene.primates_consensus_species*.Renderable.Opacity", 1.0)
      end
    ]],
    Documentation = [[Turn on and off Primates Consensus images]],
    GuiPath = "/ConsensusImages",
    IsLocal = true
  }
  asset.export("consensusImages", consensusImages.Identifier)
  
  asset.onInitialize(function()
      openspace.action.registerAction(consensusImages)
    end)
  
    asset.meta = {
      Name = "Actions - Toggle current Trails",
      Version = "1.0",
      Description = "Asset providing actions to toggle trails",
      Author = "OpenSpace Team",
      URL = "http://openspaceproject.com",
      License = "MIT license"
    }
  