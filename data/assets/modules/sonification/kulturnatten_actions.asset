  openspace.action.removeAction(ShowArt)
  openspace.action.removeAction(enable_cosmicdata)
  openspace.action.removeAction(enable_birds_lineage_29)
  openspace.action.removeAction(enable_mMDS_xyz_3Dprojection)
  openspace.action.removeAction(enable_birds_mMDS_xyz_sphere_3Dprojection)
  openspace.action.removeAction(fade_in)
  openspace.action.removeAction(fade_out)
  openspace.action.removeAction(enable_birds_mMDS_xy_3Dprojection)
  openspace.action.removeAction(enable_birds_UMAP_xyz_3Dprojection)
  openspace.action.removeAction(ShowBirdConstArt)
  openspace.action.removeAction(enable_birds_mMDS_xyz_sphere_V2_lineage_29_3Dprojection)
  openspace.action.removeAction(ShowSwan)
  openspace.action.removeAction(ShowGalli)
  openspace.action.removeAction(ShowOstrich)
  openspace.action.removeAction(ShowAllImages)

  
end)

