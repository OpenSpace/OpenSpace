  openspace.action.registerAction(ShowGalli)
  openspace.action.registerAction(ShowOstrich)
  openspace.action.registerAction(ShowAllImages)


  openspace.addScreenSpaceRenderable(treeoflife_image)
  openspace.addScreenSpaceRenderable(treeoflifeEdited_image)
  openspace.addScreenSpaceRenderable(qrkulturnatten_image)
  

  


