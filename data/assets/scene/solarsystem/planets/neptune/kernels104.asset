local kernels = asset.resource({
  Name = "Neptune Spice Kernels",
  Type = "HttpSynchronization",
  Identifier = "neptune_kernels",
  Version = 2
})


local ID = {
  Halimede = "HALIMEDE",
  Laomedeia = "LAOMEDEIA",
  Psamathe = "PSAMATHE",
  Sao = "SAO",
  Neso = "NESO",
  S2021N1 = 85051,
  S2002N5 = 85052
}


asset.onInitialize(function()
  openspace.spice.loadKernel(kernels .. "nep104.bsp")
end)

asset.onDeinitialize(function()
  openspace.spice.unloadKernel(kernels .. "nep104.bsp")
end)

asset.export("ID", ID)



asset.meta = {
  Name = "Neptune Spice Kernels",
  Description = "Generic SPICE kernels for Neptune",
  Author = "OpenSpace Team",
  URL = "https://naif.jpl.nasa.gov/pub/naif/pds/wgc/kernels/spk/",
  License = "NASA"
}
