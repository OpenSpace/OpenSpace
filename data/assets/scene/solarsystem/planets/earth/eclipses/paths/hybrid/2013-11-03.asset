local earth = asset.require("scene/solarsystem/planets/earth/earth")
local data = asset.require("../hybrid_data")



local Layer = {
  Identifier = "Hybrid-2013-11-03",
  Enabled = false,
  File = data.path .. "2013-11-03.geojson",
  HeightOffset = 2000.0,
  DefaultProperties = {
    LineWidth = 2.0,
    Extrude = true
  }
}


asset.onInitialize(function()
  openspace.globebrowsing.addGeoJson(earth.Earth.Identifier, Layer)
end)

asset.onDeinitialize(function()
  openspace.globebrowsing.deleteGeoJson(earth.Earth.Identifier, Layer)
end)



asset.meta = {
  Name = "Hybrid 2013-11-03",
  Description = [[Hybrid eclipse map for 2013-11-03. The GeoJSON is adapted from the
    data provided by Xavier M. Jubier (http://xjubier.free.fr).]],
  Author = "OpenSpace Team",
  URL = "http://openspaceproject.com",
  License = "MIT license"
}
