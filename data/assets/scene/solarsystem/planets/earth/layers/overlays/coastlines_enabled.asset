local assetHelper = asset.require("util/asset_helper")
local globeIdentifier = asset.require("./../../earth").Earth.Identifier

local layer = asset.require("./coastlines").layer
layer.Enabled = true

assetHelper.registerLayerAndExport(asset, layer, globeIdentifier, "Overlays")


asset.meta = {
  Name = "Earth Coastlines Layer (Enabled)",
  Version = "1.0",
  Description = [[ Enabled coastlines layer for Earth globe. This is the same as the 
  coastlines asset only here the layer is enabled by default.]],
  Author = "OpenSpace Team",
  URL = "http://openspaceproject.com",
  License = "MIT license"
}
