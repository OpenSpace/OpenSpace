local assetHelper = asset.require("util/asset_helper")
local mapServiceConfigsPath = asset.localResource("./../../map_service_configs")
local globeIdentifier = asset.require("./../../earth").Earth.Identifier

local layer = {
  Identifier = "Reference_Labels",
  Name = "Reference Labels",
  FilePath = mapServiceConfigsPath .. "/GIBS/overlays/Reference_Labels.wms"
}

assetHelper.registerLayerAndExport(asset, layer, globeIdentifier, "Overlays")


asset.meta = {
  Name = "Earth Reference Labels",
  Version = "1.0",
  Description = [[ Reference Labels layer for Earth globe.]],
  Author = "OpenSpace Team",
  URL = "http://openspaceproject.com",
  License = "MIT license"
}
