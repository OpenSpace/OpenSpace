local assetHelper = asset.require("util/asset_helper")
local globeIdentifier = asset.require("../../dione").Dione.Identifier

local texturesPath = asset.syncedResource({
  Name = "Dione textures",
  Type = "HttpSynchronization",
  Identifier = "dione_textures",
  Version = 1
})

local layer = {
  Identifier = "Texture",
  FilePath = texturesPath .. "dione.jpg",
}

assetHelper.registerLayerAndExport(asset, layer, globeIdentifier, "ColorLayers")


asset.meta = {
  Name = "Dione Texutre",
  Version = "1.0",
  Description = [[ Default jpg texture for Dione]],
  Author = "OpenSpace Team",
  URL = "http://openspaceproject.com",
  License = "MIT license"
}
