asset.onInitialize(function()
  openspace.spice.loadKernel(kernels.sat415)
  openspace.spice.loadKernel(kernels.sat441)
  openspace.spice.loadKernel(kernels.sat452)


  openspace.addSceneGraphNode(Aegaeon)
  openspace.addSceneGraphNode(AegaeonTrail)
  openspace.addSceneGraphNode(AegaeonLabel)

  openspace.addSceneGraphNode(Methone)
  openspace.addSceneGraphNode(MethoneTrail)
  openspace.addSceneGraphNode(MethoneLabel)

  openspace.addSceneGraphNode(Anthe)
  openspace.addSceneGraphNode(AntheTrail)
  openspace.addSceneGraphNode(AntheLabel)

  openspace.addSceneGraphNode(Pallene)
  openspace.addSceneGraphNode(PalleneTrail)
  openspace.addSceneGraphNode(PalleneLabel)

  openspace.addSceneGraphNode(Telesto)
  openspace.addSceneGraphNode(TelestoTrail)
  openspace.addSceneGraphNode(TelestoLabel)

  openspace.addSceneGraphNode(Calypso)
  openspace.addSceneGraphNode(CalypsoTrail)
  openspace.addSceneGraphNode(CalypsoLabel)

  openspace.addSceneGraphNode(Helene)
  openspace.addSceneGraphNode(HeleneTrail)
  openspace.addSceneGraphNode(HeleneLabel)

  openspace.addSceneGraphNode(Polydeuces)
  openspace.addSceneGraphNode(PolydeucesTrail)
  openspace.addSceneGraphNode(PolydeucesLabel)

  openspace.addSceneGraphNode(S2004S24)
  openspace.addSceneGraphNode(S2004S24Trail)
  openspace.addSceneGraphNode(S2004S24Label)
end)

asset.onDeinitialize(function()
  openspace.removeSceneGraphNode(AegaeonLabel)
  openspace.removeSceneGraphNode(AegaeonTrail)
  openspace.removeSceneGraphNode(Aegaeon)

  openspace.removeSceneGraphNode(MethoneLabel)
  openspace.removeSceneGraphNode(MethoneTrail)
  openspace.removeSceneGraphNode(Methone)

  openspace.removeSceneGraphNode(AntheLabel)
  openspace.removeSceneGraphNode(AntheTrail)
  openspace.removeSceneGraphNode(Anthe)

  openspace.removeSceneGraphNode(PalleneLabel)
  openspace.removeSceneGraphNode(PalleneTrail)
  openspace.removeSceneGraphNode(Pallene)

  openspace.removeSceneGraphNode(TelestoLabel)
  openspace.removeSceneGraphNode(TelestoTrail)
  openspace.removeSceneGraphNode(Telesto)

  openspace.removeSceneGraphNode(CalypsoLabel)
  openspace.removeSceneGraphNode(CalypsoTrail)
  openspace.removeSceneGraphNode(Calypso)

  openspace.removeSceneGraphNode(HeleneLabel)
  openspace.removeSceneGraphNode(HeleneTrail)
  openspace.removeSceneGraphNode(Helene)

  openspace.removeSceneGraphNode(PolydeucesLabel)
  openspace.removeSceneGraphNode(PolydeucesTrail)
  openspace.removeSceneGraphNode(Polydeuces)

  openspace.removeSceneGraphNode(S2004S24Label)
