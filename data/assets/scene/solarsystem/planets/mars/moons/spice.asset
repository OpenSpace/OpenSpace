local base = asset.require("spice/base")



local kernels = asset.resource({
  Name = "Mars Spice Kernels",
  Type = "HttpSynchronization",
  Identifier = "mars_kernels",
  Version = 1
})


local ID = {
  Deimos = "DEIMOS",
  Phobos = "PHOBOS"
}

local Frame = {
  Deimos = base.Frame.Deimos,
  Phobos = base.Frame.Phobos
}


asset.onInitialize(function()
  openspace.spice.loadKernel(kernels .. "mar097.bsp")
end)

asset.onDeinitialize(function()
  openspace.spice.unloadKernel(kernels .. "mar097.bsp")
end)

asset.export("ID", ID)
asset.export("Frame", Frame)
