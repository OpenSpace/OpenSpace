local assetHelper = asset.require("util/asset_helper")
local globeIdentifier = asset.require("../../io").Io.Identifier

local texturesPath = asset.syncedResource({
  Name = "Io Textures",
  Type = "HttpSynchronization",
  Identifier = "io_textures",
  Version = 1
})

local layer = {
  Identifier = "Texture",
  FilePath = texturesPath .. "io.jpg",
}

assetHelper.registerLayerAndExport(asset, layer, globeIdentifier, "ColorLayers")

asset.meta = {
  Name = "Io Texutre",
  Version = "1.0",
  Description = [[ Default jpg texture for Io]],
  Author = "OpenSpace Team",
  URL = "http://openspaceproject.com",
  License = "MIT license"
}
