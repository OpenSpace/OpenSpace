local layer = {
  Identifier = "Texture",
  FilePath = texturesPath .. "/ganymede.jpg",
}

assetHelper.registerLayerAndExport(asset, layer, globeIdentifier, "ColorLayers")


asset.meta = {
  Name = "Ganymede Texutre",
  Version = "1.0",
  Description = [[ Default jpg texture for Ganymede]],
  Author = "OpenSpace Team",
