local distance = {
  Type = "DashboardItemDistance",
  Identifier = "GaiaEarthDistance",
  GuiName = "Gaia Earth Distance",
  SourceType = "Node",
  SourceNodeName = "Gaia",
  DestinationType = "Node Surface",
  DestinationNodeName = "Earth"
}

dashboardAsset.onInitialize(function ()
  openspace.dashboard.addDashboardItem(distance)
end)

dashboardAsset.onDeinitialize(function ()
  openspace.dashboard.removeDashboardItem(distance.Identifier)
end)
