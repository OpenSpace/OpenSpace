local setup_launch = {
  Identifier = "os.missoins.artemis.setup.launch",
  Name = "Set to Artemis-1 launch time",
  Command = [[
    openspace.time.setTime('2022-11-16T08:19:00.000');
  ]],
  Documentation = "Set the time to the launch time of Artemis-1",
  GuiPath = "/Artemis",
  IsLocal = false
}

local flyto_artemis = {
  Identifier = "os.missoins.artemis.flyto.artemis",
  Name = "Fly to Artemis-1",
  Command = [[
    openspace.pathnavigation.flyTo("ArtemisModel")
  ]],
  Documentation = "Fly to the Orion capsule (Artemis-1) model",
  GuiPath = "/Artemis",
  IsLocal = false
}

asset.onInitialize(function()
  openspace.action.registerAction(setup_launch)
  openspace.action.registerAction(flyto_artemis)
end)

asset.onDeinitialize(function()
  openspace.action.removeAction(flyto_artemis)
  openspace.action.removeAction(setup_launch)
end)

asset.export("SetupLaunch", setup_launch)
asset.export("FlyToArtemis", flyto_artemis)

asset.meta = {
  Name = "Actions - Artemis",
  Version = "1.0",
  Description = "Actions related to the Artemis mission",
  Author = "OpenSpace Team",
  URL = "http://openspaceproject.com",
  License = "MIT license"
}
