</test>
<test>
  <op name="differenceNG" arg1="A" arg2="B">
    POLYGON ((10 10, 10 30, 30 30, 30 10, 10 10), (15 15, 25 15, 15 25, 15 15))
  </op>
</test>
<test>
  <op name="symdifferenceNG" arg1="A" arg2="B">
    POLYGON ((10 10, 10 30, 30 30, 30 10, 10 10), (15 15, 25 15, 15 25, 15 15))
  </op>
</test>
</case>

